		<object-name>GalleyPwrBtn2O</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<condition>
					<greater-than-equals>
						<property>systems/electrical/bus/dc-service-bus</property>
						<value>25</value>
